{
    "name": "ezhasyafaat/purwantara-laravel",
    "description": "Package for use payment in purwantara.id",
    "type": "library",
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "php": "^7.0|^8.0",
        "friendsofphp/php-cs-fixer": "^3.11"
    },
    "require-dev": {
        "orchestra/testbench": "^7.7",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ezhasyafaat\\PurwantaraPayment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ezhasyafaat\\PurwantaraPayment\\PurwantaraServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Muh Ezha Syafaat",
            "email": "ezhasyafaat@gmail.com"
        }
    ],
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes"
    }
}
